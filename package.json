  "homepage": "https://github.com/distributeaid/covid-19-resources-drive-export#readme",
  "dependencies": {
    "algoliasearch": "4.4.0",
    "feather-icons": "4.28.0",
    "gatsby-plugin-algolia": "0.11.2",
    "gatsby-remark-autolink-headers": "2.3.12",
    "googleapis": "59.0.0",
    "jsdom": "16.4.0",
    "react-instantsearch-dom": "6.7.0",
    "snyk": "1.392.0",
    "use-debounce": "4.0.0"
  },
  "snyk": true,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=6.0.0"
  },
  "devDependencies": {
    "@bifravst/code-style": "8.0.66",
    "@bifravst/eslint-config-typescript": "5.0.28",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-angular": "9.1.2",
