    "prettier": "2.1.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "sanitize-filename": "1.6.3",
    "styled-components": "5.2.0",
    "ts-node": "9.0.0",
    "typescript": "4.0.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.tsx"
    ],
    "*.{md,json,yaml,yml,js}": [
